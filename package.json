{
  "name": "expo-workspaces",
  "private": true,
  "cacheDirectories": [
    "ignored"
  ],
  "version": "1.0.0",
  "pm": "yarn@3.0.0-rc.2",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "start:server": "yarn workspace @expo-workspaces/api start",
    "start:android": "expo start --android",
    "start:ios": "expo start --ios",
    "start:web": "expo start --web",
    "eject": "expo eject",
    "postinstall": "expo-yarn-workspaces postinstall",
    "lint": "eslint --fix ."
  },
  "keywords": [
    "expo",
    "yarn",
    "workspaces"
  ],
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@eva-design/eva": "^2.1.0",
    "@ui-kitten/components": "^5.1.0",
    "@ui-kitten/eva-icons": "^5.1.0",
    "add": "^2.0.6",
    "expo": "~41.0.1",
    "expo-status-bar": "~1.0.4",
    "google-protobuf": "^3.17.1",
    "grpc-web": "^1.2.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-error-boundary": "^3.1.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-41.0.0.tar.gz",
    "react-native-safe-area-context": "3.2.0",
    "react-native-svg": "12.1.0",
    "react-native-web": "~0.13.12",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@expo/webpack-config": "^0.12.73",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-module-resolver": "^1.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "expo-yarn-workspaces": "^1.5.1",
    "prettier": "^2.3.0",
    "typescript": "~4.0.0"
  }
}
